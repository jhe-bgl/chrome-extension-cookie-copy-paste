{
  "manifest_version": 3,
  "name": "URL Block Extension",
  "description": "Block Urls configured in the extension",
  "version": "1.0.1",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon-48.png"
  }
}
