{
  "manifest_version": 3,
  "name": "Copy Paste Cookie Extension",
  "version": "1.0",
  "description": "Copies and paste cookie value from an website to another.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "cookies",
    "clipboardWrite",
    "clipboardRead"
  ],
  "host_permissions": [
  "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon-48.png"
    }
  },
  "icons": {
    "48": "icon-48.png"
  }
}